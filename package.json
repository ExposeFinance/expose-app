{
  "name": "expose-app",
  "private": false,
  "license": "Apache-2.0",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "yarn download:styles && vite --host",
    "build": "yarn download:styles && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "yarn vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "download:styles": "curl -L 'https://raw.githubusercontent.com/Yield-Fi/joinn-design-system/main/build/tailwind/base.css' -o 'src/styles/css/base.css' && curl -L 'https://raw.githubusercontent.com/Yield-Fi/joinn-design-system/main/build/tailwind/dark.css' -o 'src/styles/css/dark.css' && curl -L 'https://raw.githubusercontent.com/Yield-Fi/joinn-design-system/main/build/tailwind/tailwind.base.js' -o 'tailwind.base.js'"
  },
  "dependencies": {
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "aos": "^2.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.3.1",
    "lucide-react": "^0.468.0",
    "openai": "^4.82.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-lottie-player": "^2.1.0",
    "react-rewards": "^2.0.4",
    "react-router-dom": "^7.0.2",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "thirdweb": "^5.84.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/aos": "^3.0.7",
    "@types/node": "^22.10.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "style-dictionary": "^4.3.2",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^2.1.8"
  }
}
